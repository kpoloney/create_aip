{
  "BagIt-Profile-Info": {
    "BagIt-Profile-Identifier":"ark:/to/profile",
    "Source-Organization":"Simon Fraser University",
    "External-Description":"BagIt Profile for AIPs created for SFU Library digital repositories.",
    "Version":"0.9",
    "BagIt-Profile-Version":"1.3.0"
  },
  "Bag-Info": {
    "Source-Organization": {
      "required": true,
      "values": "Simon Fraser University"
    },
    "Contact-Name": {
      "required": false
    },
    "Contact-Phone": {
      "required": false
    },
    "Contact-Email": {
      "required": true,
      "values": [
        "summit@sfu.ca",
        "libhelp@sfu.ca"
      ]
    },
    "Organization-Address": {
      "required": true,
      "values": "8888 University Dr, Burnaby, BC V5A 1S6"
    },
    "Bagging-Date": {
      "required": true,
      "repeatable": false
    },
    "External-Identifier": {
      "required": true,
      "description": "Value is the ARK for the object."
    },
    "External-Description":{
      "required": true,
      "description": "Use either the ERC 'what' value or the Islandora title."
    },
    "Internal-Sender-Identifier": {
      "required": true,
      "description": "Value is either the Islandora URL for the object or the local filepath where it is stored."
    },
    "Internal-Sender-Description": {
      "required": false
    },
    "Source-Repository": {
      "required": true,
      "values": [
        "Summit Research Repository",
        "Special Collections and Rare Books"
      ],
      "description": "Indicate the Library repository responsible for the object."
    },
    "Bag-Size": {
      "required": true,
      "repeatable": false
    },
    "Payload-Oxum": {
      "required": true,
      "repeatable": false
    },
    "Validation-Date": {
      "required": true,
      "description": "Date in format YYYY-MM-DD on which the bag was validated."
    }
  },
  "Manifests-Required": [
    "sha256",
    "md5"
  ],
  "Manifests-Allowed": [
    "sha256",
    "md5",
    "sha512"
  ],
  "Serialization": "optional",
  "Accept-Serialization": [
    "application/zip"
  ],
  "Accept-BagIt-Version": [
    "0.97",
    "1.0"
  ],
  "Tag-Manifests-Required": [
    "sha256",
    "md5"
  ],
  "Tag-Manifests-Allowed": [
    "sha256",
    "md5",
    "sha512"
  ]

}